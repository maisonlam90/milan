{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            lt.id,\n            lt.transaction_type,\n            lt.amount,\n            lt.date,\n            lt.note,\n            lc.contract_number,\n            c.name as contact_name\n        FROM loan_transaction lt\n        JOIN loan_contract lc ON lt.contract_id = lc.id\n        JOIN contact c ON lc.contact_id = c.id\n        WHERE lt.tenant_id = $1\n        ORDER BY lt.date DESC, lt.created_at DESC\n        LIMIT 5\n        ",
